{
  "name": "next-styled-components-template",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && node scripts/lighthouse/index.js $HUSKY_GIT_STDIN"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "build-export": "npm run build && npm run export",
    "serve": "npm run build-export && serve out",
    "lint": "tsc --noEmit -p .",
    "pm2": "npm run build-export && pm2 start ecosystem.config.js"
  },
  "dependencies": {
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.5",
    "babel-plugin-styled-components": "^1.12.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.6",
    "imagemin-webp-webpack-plugin": "^3.3.3",
    "lighthouse": "^6.5.0",
    "serve": "^11.3.2",
    "serve-handler": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
